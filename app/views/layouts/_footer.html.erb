<footer class="footer">
<div class="container">
<% [:notice, :error, :alert, :success].each do |level| %>
  <% unless flash[level].blank? %>
    <div class="<%= flash_class(level) %>">
      <%= content_tag :span, flash[level] %>
    </div>
  <% end %>
<% end %>
</div>
<div class="container">
  <small>
    <a href="http://chores.com/">Chores</a>
  </small>
  <nav>
    <ul>
      <li><%= link_to "About",   '#' %></li>
      <li><%= link_to "Contact", '#' %></li>
      <li><a href="http://news.chores.com/">News</a></li>
    </ul>
  </nav>

</div>
</footer>
